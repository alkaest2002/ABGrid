{
  "name": "webapp",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "dev": "vite --port 53472",
    "build": "vite build --emptyOutDir",
    "build:electron": "npm run build && npm run copy:dist && npm run copy:app",
    "copy:dist": "rm -rf ../electron-thin/app/dist && cp -r dist ../electron-thin/app/dist",
    "copy:app": "rm -rf ../electron-thin/Electron.app/Contents/Resources/app &&  cp -r ../electron-thin/app ../electron-thin/Electron.app/Contents/Resources/app",
    "preview": "vite preview"
  },
  "devDependencies": {
    "@vitejs/plugin-basic-ssl": "^2.1.0",
    "vite": "^6.3.5"
  },
  "dependencies": {
    "@alpinejs/persist": "^3.14.9",
    "@tailwindcss/vite": "^4.1.11",
    "@unseenco/taxi": "^1.9.0",
    "alpinejs": "^3.14.9",
    "alpinejs-i18n": "^2.5.3",
    "tailwindcss": "^4.1.11",
    "yaml": "^2.8.0"
  }
}
